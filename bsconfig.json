{
  "name": "dp-webapp",
  "reason": {
    "react-jsx": 3
  },
  "sources": [
    {
      "dir": "src",
      "subdirs": true,
      "generators": [
        {
          "name": "gen-tailwind",
          "edge": ["tailwind.css", ":", "index.css"]
        }
      ]
    },
    {
      "dir": "__tests__",
      "type": "dev"
    }
  ],
  "generators": [
    {
      "name": "gen-tailwind",
      "command": "tailwindcss build $in -o $out -c ../../tailwind.config.js"
    }
  ],
  "bsc-flags": ["-bs-super-errors", "-bs-no-version-header"],
  "ppx-flags": [
    "bs-let/ppx",
    "re-formality/ppx",
    "decco/ppx",
    ["@dylanirlbeck/tailwind-ppx/tailwind-ppx", "-path ./src/tailwind.css"]
  ],
  "package-specs": [
    {
      "module": "commonjs",
      "in-source": true
    }
  ],
  "suffix": ".bs.js",
  "namespace": false,
  "bs-dependencies": [
    "bs-webapi",
    "reason-react",
    "re-classnames",
    "re-formality",
    "decco"
  ],
  "bs-dev-dependencies": [
    "reason-test-framework",
    "bs-react-testing-library",
    "bs-dom-testing-library"
  ],
  "refmt": 3,
  "gentypeconfig": {
    "language": "typescript",
    "shims": {
      "Js": "Js",
      "React": "ReactShim",
      "ReactEvent": "ReactEvent",
      "ReasonReact": "ReactShim"
    },
    "debug": {
      "all": false,
      "basic": false
    }
  }
}
